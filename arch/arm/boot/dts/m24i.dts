/dts-v1/;
#include "at91sam9g35.dtsi"
#include "at91sam9x5dm.dtsi"
#include "at91sam9x5ek.dtsi"

/ {
	model = "Cryocon M24I";
	compatible = "atmel,at91sam9g35ek", "atmel,at91sam9x5ek", "atmel,at91sam9x5", "atmel,at91sam9";

	ahb {
		apb {
			hlcdc: hlcdc@f8038000 {
				status = "okay";
				hlcdc-display-controller {
					pinctrl-0 = <&pinctrl_lcd_base &pinctrl_lcd_rgb565>;
				};
			};

			macb0: ethernet@f802c000 {
				phy-mode = "rmii";
				status = "okay";
				phy-reset-gpios = <&pioA 3 0>; /* PA3 GPIO phy reset */

                pinctrl-0 = <&pinctrl_macb0_rmii>;
			};

			adc0: adc@f804c000 {
				status = "okay";
			};

			spi0: spi@f0000000 {
				status = "okay";
				cs-gpios = <&pioA 14 0>, <&pioA 7 0>, <0>, <0>;
                spidev@0 {
                    compatible = "spidev";
                    spi-max-frequency = <1000000>;
                    reg = <0>;
                };
                spidev@1 {
                    compatible = "spidev";
                    spi-max-frequency = <1000000>;
                    reg = <1>;
                };
			};

			spi1: spi@f0004000 {
			    status = "okay";
                cs-gpios = <&pioA 8 0>, <&pioA 0 0>, <&pioD 21 0>, <0>;
                spidev@0 {
                    compatible = "spidev";
                    spi-max-frequency = <3000000>;
                    reg = <0>;
                };
                spidev@1 {
                    compatible = "spidev";
                    spi-max-frequency = <1000000>;
                    reg = <1>;
                };
                spidev@2 {
                	status = "disabled";
                    compatible = "spidev";
                    spi-max-frequency = <1000000>;
                    reg = <2>;
                };
            };	
            
			mmc0: mmc@f0008000 {
				pinctrl-0 = <
					&pinctrl_mmc0_slot0_clk_cmd_dat0
					&pinctrl_mmc0_slot0_dat1_3>;
				status = "okay";
				slot@0 {
					reg = <0>;
					bus-width = <4>;
					cd-gpios = <0>;
				};
			};
			
			mmc1: mmc@f000c000 {
				status = "disabled";
			};
			
			dbgu: serial@fffff200 {
				status = "okay";
			};

			usart1: serial@f8020000 {
				status = "okay";
			};
			
			i2c0: i2c@f8010000 {
				status = "okay";
				lm73@48 {
					compatible = "lm73";
					reg = <0x48>;
				};
			};
			
			pinctrl@fffff400 {
				fpga0 {
				    pinctrl_board_fpga0: fpga0-board {
				        atmel,pins =
				            <AT91_PIOA 13 AT91_PERIPH_GPIO AT91_PINCTRL_NONE 
				             AT91_PIOD 16 AT91_PERIPH_GPIO AT91_PINCTRL_NONE 
				             AT91_PIOD 17 AT91_PERIPH_GPIO AT91_PINCTRL_NONE 
				             AT91_PIOD 15 AT91_PERIPH_GPIO AT91_PINCTRL_NONE 
				             AT91_PIOA 12 AT91_PERIPH_GPIO AT91_PINCTRL_NONE>;
				     };
				};
				fpga1 {
				    pinctrl_board_fpga1: fpga1-board {
				        atmel,pins =
				            <AT91_PIOA 23 AT91_PERIPH_GPIO AT91_PINCTRL_NONE 
				             AT91_PIOC  1 AT91_PERIPH_GPIO AT91_PINCTRL_NONE 
				             AT91_PIOC  2 AT91_PERIPH_GPIO AT91_PINCTRL_NONE 
				             AT91_PIOC  0 AT91_PERIPH_GPIO AT91_PINCTRL_NONE 
				             AT91_PIOA 22 AT91_PERIPH_GPIO AT91_PINCTRL_NONE>;
				     };
				};
				nand0 {
					pinctrl_nand0_m18: nand0_m18 {
						atmel,pins =
							<3 14 0x0 0x1>;	/* PD14 WP# pull up */
					};
				};
			};

			
			fpga0: xilinx-sscu@0 {
			    compatible = "xilinx-sscu";
                status = "okay";
                pinctrl-names = "default";
                pinctrl-0 = <&pinctrl_board_fpga0>;
                gpios = <&pioA 13 0
                        &pioD 16 0
                        &pioD 17 0
                        &pioD 15 0
                        &pioA 12 0>;
            };
            
            fpga1: xilinx-sscu@0 {
			    compatible = "xilinx-sscu";
                status = "okay";
                pinctrl-names = "default";
                pinctrl-0 = <&pinctrl_board_fpga1>;
                gpios = <&pioA 23 0
                        &pioC  1 0
                        &pioC  2 0
                        &pioC  0 0
                        &pioA 22 0>;
            };

			ssc0: ssc@f0010000 {
				status = "disabled";
			};
		};
	};
	
	leds {
		compatible = "gpio-leds";

		heartbeat {
			label = "led1_ct";
			gpios = <&pioC 9 1>;
			linux,default-trigger = "heartbeat";
		};
		
		fan {
			label = "fan";
			gpios = <&pioC 8 0>;
			linux,default-trigger = "default-on";
		};
	};
	
	gpio_keys {
		compatible = "gpio-keys";
		#address-cells = <1>;
		#size-cells = <0>;

		key_interrupt {
			label = "key_interrupt";
			gpios = <&pioD 18 1>;
			linux,code = <103>;
			gpio-key,wakeup;
		};
		
		key_select {
			label = "key_select";
			gpios = <&pioA 25 1>;
			linux,code = <0x161>;
			gpio-key,wakeup;
		};
	};
	
	bl_reg: backlight_regulator {
		status = "disabled";
	};

	panel_reg: panel_regulator {
		status = "disabled";
	};

	backlight: backlight {
		status = "disabled";
	};

	panel: panel {
		status = "disabled";
	};
};
