/dts-v1/;
/include/ "at91sam9g35.dtsi"

/ {

    model = "Cryocon M18I-B";
    compatible = "atmel,at91sam9x5ek", "atmel,at91sam9x5", "atmel,at91sam9";
    
	aliases {
		serial1 = &usart1;
	};

    memory {
        reg = <0x20000000 0x8000000>;
    };

    clocks {
        #address-cells = <1>;
        #size-cells = <1>;
        ranges;

        main_clock: clock@0 {
            compatible = "atmel,osc", "fixed-clock";
            clock-frequency = <12000000>;
        };
    };

	chosen {
		bootargs = "console=ttyS0,115200 ubi.mtd=rootfs rootfstype=ubifs root=ubi0_0";
	};

    aliases {
        spi0 = &spi0;
        spi1 = &spi1;
    };

	ahb {
		nand0: nand@40000000 {
			nand-bus-width = <8>;
			nand-ecc-mode = "hw";
			atmel,has-pmecc;	/* Enable PMECC */
			atmel,pmecc-cap = <2>;
			atmel,pmecc-sector-size = <512>;
			nand-on-flash-bbt;
			status = "okay";

            pinctrl-names = "default";
            pinctrl-0 = <&pinctrl_nand0_m54>;
			at91bootstrap@0 {
				label = "at91bootstrap";
				reg = <0x0 0x40000>;
			};

			uboot@40000 {
				label = "u-boot";
				reg = <0x40000 0x80000>;
			};

			ubootenv@c0000 {
				label = "U-Boot Env";
				reg = <0xc0000 0x140000>;
			};

			kernel@200000 {
				label = "kernel";
				reg = <0x200000 0x600000>;
			};

			rootfs@800000 {
				label = "rootfs";
				reg = <0x800000 0xf780000>;
			};
		};
			
		apb {
			
			mmc1: mmc@f000c000 {
				status = "disabled";
			};

			dbgu: serial@fffff200 {
				status = "okay";
			};

			usart1: serial@f8020000 {
				status = "okay";
			};

			i2c0: i2c@f8010000 {
				status = "okay";
				24c512@50 {
					compatible = "24c512";
					reg = <0x50>;
					pagesize = <128>;
				};		
				lm73@49 {
					compatible = "lm73";
					reg = <0x49>;
				};
			};

			pinctrl@fffff400 {
				board {
					pinctrl_mmc0_cd: mmc0_cd {
						atmel,pins =
							<3 15 0x0 0x5>;	/* PD15 gpio with pullup deglitch */
					};
				};

				macb0 {
					pinctrl_macb0_m54_phy_rst_n: macb0_m54_phy_rst_n-0 {
			                        atmel,pins = <0 3 0x0 0x1>;    /* PA3 gpio NRST_PHY pull up */
                    			};
				};

				nand0 {
					pinctrl_nand0_m54: nand0_m54-0 {
						atmel,pins =
							<3 14 0x0 0x1	/* PD14 CE# pull up */
							 3 18 0x0 0x1>;	/* PD18 WP# pull up */
					};
				};
			};

			mmc0: mmc@f0008000 {
				pinctrl-0 = <&pinctrl_mmc0_clk_cmd_dat0 &pinctrl_mmc0_dat1_3 &pinctrl_mmc0_cd>;
				status = "okay";
				slot@0 {
					reg = <0>;
					bus-width = <4>;
					cd-gpios = <&pioD 15 0>;
				};
			};

            macb0: ethernet@f802c000 {
                phy-mode = "rmii";
                status = "okay";

                pinctrl-0 = <&pinctrl_macb0_rmii
                             &pinctrl_macb0_m54_phy_rst_n>;
            };

			spi0: spi@f0000000 {
				status = "okay";
				cs-gpios = <&pioA 14 0>, <&pioA 7 0>, <&pioA 1 0>, <0>;
                spidev@0 {
                    compatible = "spidev";
                    spi-max-frequency = <1000000>;
                    reg = <0>;
                };
			};

			spi1: spi@f0004000 {
			    status = "okay";
                cs-gpios = <&pioA 8 0>, <&pioA 0 0>, <0>, <0>;
                spidev@0 {
                    compatible = "spidev";
                    spi-max-frequency = <3000000>;
                    reg = <0>;
                };
            };

		};

		usb0: ohci@00600000 {
			status = "disabled";
		};

		usb1: ehci@00700000 {
			status = "disabled";
		};
	};
	
	leds {
		compatible = "gpio-leds";

		pb18 {
			label = "led1_ct";
			gpios = <&pioC 9 1>;
			linux,default-trigger = "heartbeat";
		};
		
		};
		

	
};
